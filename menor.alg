algoritmo "semnome"
// Função :
// Autor :
// Data : 18/03/2021
// Seção de Declarações
var
a, b, c : inteiro
inicio
escreva("Primeiro Valor:")
leia(a)
escreva("Segundo Valor: ")
leia(b)
escreva("Terceiro Valor: ")
leia(c)
se a < b entao
   escreva("MENOR= ", a)
senao
   se b < a entao
      escreva("MENOR= ", b)
   senao
      escreva("MENOR= ",c)
   FimSe
FimSe
fimalgoritmo
